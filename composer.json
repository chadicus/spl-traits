{
    "name": "chadicus/spl-traits",
    "description": "A collection standard traits",
    "keywords": ["spl", "traits"],
    "authors": [
        {
            "name": "Chad Gray",
            "email": "chadwickgray@gmail.com",
            "homepage": "http://chadicus.org",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~5.4 || ~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "chadicus/coding-standard": "^1.1",
        "satooshi/php-coveralls": "~0.6.1"
    },
    "autoload": {
        "psr-4": { "Chadicus\\Spl\\Traits\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "ChadicusTest\\Spl\\Traits\\": "tests" }
    }
}
